Host github.com
  User bwhour
  Hostname github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa

Host gitlab.com
  HostName altssh.gitlab.com
  Port 443
  # TODO: Replace with your GitLab username
  User your-gitlab-username
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gitlab

Host p1
 HostName bighelp.io
 # SECURITY WARNING: StrictHostKeyChecking no disables host key verification
 # This makes you vulnerable to MITM attacks. Use only on trusted networks.
 # Consider using: StrictHostKeyChecking accept-new (for first connection) or yes (strict)
 StrictHostKeyChecking no
 # HostKeyAlias allows using a custom alias for host key verification
 # Replace 'mytest' with a meaningful alias if needed
 HostKeyAlias mytest
 Port 22
 # TODO: Replace with your server username
 User your-server-username
 ForwardAgent yes
 TCPKeepAlive no
 ControlMaster auto
 ControlPath ~/.ssh/control/%C
 ControlPersist 4h
 ServerAliveInterval 120
 ServerAliveCountMax 5
 Compression yes
