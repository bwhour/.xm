  IdentityFile ~/.ssh/id_rsa_gitlab

Host p1
 HostName xxx.io
 StrictHostKeyChecking no
 HostKeyAlias mytest
 Port 22
 User xxx.xxx
 ForwardAgent yes
 TCPKeepAlive no
 ControlMaster auto
 ControlPath ~/.ssh/control/%C
 ControlPersist 4h
 ServerAliveInterval 120
 ServerAliveCountMax 5
 Compression yes

